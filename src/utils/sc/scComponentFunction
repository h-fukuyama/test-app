export const replaceValue = (value) => {
    switch (value) {
      case '00':
        return "電源ON⇔OFF";
      case '01':
        return "電源ON";
      case '02':
        return "電源OFF";
      default:
        return value; // 何も該当しない場合は元の値をそのまま返す
    }
};

export const generateOutput = (input) => {
    const settingValue = {
        '00': 'カット⇔解除',
        '01': 'カット',
        '02': '解除',
    }[input] || '不明';
    return settingValue;
};